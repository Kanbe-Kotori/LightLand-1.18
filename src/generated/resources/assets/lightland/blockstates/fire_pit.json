{
  "multipart": [
    {
      "when": {
        "lit": "true"
      },
      "apply": {
        "model": "lightland:block/pan_lit"
      }
    },
    {
      "when": {
        "facing": "north"
      },
      "apply": {
        "model": "lightland:block/fire_pit",
        "y": 180
      }
    },
    {
      "when": {
        "facing": "east"
      },
      "apply": {
        "model": "lightland:block/fire_pit",
        "y": 270
      }
    },
    {
      "when": {
        "facing": "south"
      },
      "apply": {
        "model": "lightland:block/fire_pit"
      }
    },
    {
      "when": {
        "facing": "west"
      },
      "apply": {
        "model": "lightland:block/fire_pit",
        "y": 90
      }
    }
  ]
}